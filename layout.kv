#:kivy 2.0

<MyLayout>:
    id: my_widget

    BoxLayout:
        orientation: 'vertical'
        size: root.width, root.height
        padding: 50
        spacing: 20

        Image:
            id: my_img
            source: ''

        FileChooserIconView:
            id: browse
            on_selection: my_widget.selected(browse.selection)
